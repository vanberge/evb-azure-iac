{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "virtualMachineName": {
            "value": null
        },
        "virtualMachineSize": {
            "value": "Standard_B2s"
        },
        "adminUsername": {
            "value": "evanbergen"
        },
        "virtualNetworkName": {
            "value": "evb-iac-vnet"
        },
        "networkInterfaceName": {
